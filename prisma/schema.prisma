generator client {
  provider = "prisma-client-js"
  output   = "./generated"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {}

model Tournaments {}

model TournamentParticipants {}

model Matches {}

model MatchResults {}

model Leaderboards {}
